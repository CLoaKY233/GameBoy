{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "boostify",
  "version": "0.1.0",
  "identifier": "boositfyv1",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "width": 1000,
        "height": 650,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["myicon.jpg"]
  }
}
